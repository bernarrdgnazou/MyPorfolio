{
  "name": "mon-portfolio",
  "version": "1.0.0",
  "description": "Un portfolio moderne et technologique créé avec Flask et des outils JavaScript modernes.",
  "main": "app.js",
  "scripts": {
    "start": "python app.py",
    "build:css": "tailwindcss -i ./static/src/styles.css -o ./static/css/styles.css --watch",
    "dev": "concurrently \"npm run build:css\" \"npm run start\""
  },
  "keywords": [
    "portfolio",
    "flask",
    "tailwindcss",
    "daisyui",
    "gsap"
  ],
  "author": "Ton Artemix",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "aos": "^2.3.4",
    "daisyui": "^3.0.0",
    "framer-motion": "^12.4.10",
    "gsap": "^3.12.7",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-text-fill-stroke": "^2.0.0-beta.3",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "tailwindcss": "^3.0.0"
  }
}
